<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 바깥쪽 테두리 -->
    <item>
        <shape
                android:shape="rectangle">
            <solid android:color="@android:color/white"/> <!-- 배경 색상 -->
            <corners android:radius="35dp"/> <!-- 원형의 반지름 (뷰의 크기의 절반) -->
            <stroke
                    android:width="3dp"
                    android:color="@color/humate_main"/>
        </shape>
    </item>
</layer-list>
